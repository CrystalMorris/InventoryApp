<h2>{{thisWarehouse.name}}</h2>

<img class="item-img" src="{{thisWarehouse.image}}" />
<h3>Inventory</h3>

    {{#each localItems}}

 
    <div class="card col-md-3">
        <h3>{{name}}</h3>
        <img class="item-img" src="{{image}}" />
